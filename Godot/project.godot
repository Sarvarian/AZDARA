; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Object",
"class": "Addresses",
"language": "GDScript",
"path": "res://aban/static_objects/addresses.gd"
}, {
"base": "Node",
"class": "Entity",
"language": "GDScript",
"path": "res://aban/entity/entity.gd"
}, {
"base": "Object",
"class": "EntityBuilder",
"language": "GDScript",
"path": "res://aban/entity/builder.gd"
}, {
"base": "Camera2D",
"class": "PlayerCamera",
"language": "GDScript",
"path": "res://aban/player_camera/player_camera.gd"
}, {
"base": "Node",
"class": "PlayerInput",
"language": "GDScript",
"path": "res://aban/entity/player_input.gd"
}, {
"base": "MarginContainer",
"class": "PlayerViewport",
"language": "GDScript",
"path": "res://aban/gui/player_viewport.gd"
} ]
_global_script_class_icons={
"Addresses": "",
"Entity": "",
"EntityBuilder": "",
"PlayerCamera": "",
"PlayerInput": "",
"PlayerViewport": ""
}

[application]

config/name="azdara"
run/main_scene="res://aban/main/main.tscn"

[debug]

settings/stdout/verbose_stdout=true

[display]

window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "smoothing" )

[physics]

common/physics_fps=5
2d/thread_model=2

[rendering]

quality/driver/driver_name="GLES2"
threads/thread_model=2
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0, 0, 0, 1 )
