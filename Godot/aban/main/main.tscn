func _input(event : InputEvent) -> void:
	if event.is_action_pressed(\"ui_accept\"):
		var state : String = $Game.state()
		var save_game = File.new()
		save_game.open(\"user://savegame.json\", File.WRITE)
		save_game.store_line(state)
		save_game.close()
		var dic : Dictionary = parse_json(state)
		var entities : Dictionary = dic.entities
#		print(dic)
		for entity in entities.values():
			var e : Dictionary = entity
			if e.has(\"player\"):
				$icon.position = Vector2(e.movable.x, e.movable.y) * 50
	pass

